{
  "name": "docs",
  "private": true,
  "version": "1.3.2",
  "scripts": {
    "start": "concurrently -n sass,app -c 'bgMagenta,bgMagenta' 'pnpm build:sass:watch' 'storybook dev -p 6006'",
    "build:copy": "cp -r ../drop-in/dist public/",
    "build:sass": "sass ./stories/assets/styles/:./public/",
    "build:sass:watch": "nodemon --watch ./node_modules/@commercelayer/drop-in.js -e scss --exec 'pnpm build:sass'",
    "build": "pnpm build:sass && pnpm build:copy && storybook build --docs && pnpm replace:title",
    "replace:title": "sed -i -e 's/Webpack App/Commerce Layer drop-in library/g' ./storybook-static/index.html",
    "serve": "http-server storybook-static -o"
  },
  "author": {
    "name": "Marco Montalbano",
    "email": "marco.montalbano@commercelayer.io"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.5",
    "@storybook/addon-actions": "^7.0.17",
    "@storybook/addon-essentials": "^7.0.17",
    "@storybook/addon-interactions": "^7.0.17",
    "@storybook/addon-links": "^7.0.17",
    "@storybook/addon-mdx-gfm": "^7.0.17",
    "@storybook/addons": "^7.0.17",
    "@storybook/api": "^7.0.17",
    "@storybook/blocks": "^7.0.17",
    "@storybook/components": "^7.0.17",
    "@storybook/core-common": "^7.0.17",
    "@storybook/html": "^7.0.17",
    "@storybook/html-webpack5": "^7.0.17",
    "@storybook/testing-library": "^0.1.0",
    "@storybook/theming": "^7.0.17",
    "@storybook/types": "^7.0.17",
    "@types/common-tags": "^1.8.1",
    "@types/react": "^18",
    "babel-loader": "^9.1.2",
    "common-tags": "^1.8.2",
    "http-server": "^14.1.1",
    "nodemon": "^2.0.22",
    "react": "^18",
    "react-dom": "^18",
    "sass": "^1.62.1",
    "storybook": "^7.0.17",
    "typescript": "^5.0.4",
    "webpack": "^5.83.1"
  },
  "dependencies": {
    "@commercelayer/drop-in.js": "workspace:^1.3.2",
    "lit-html": "^2.7.4"
  }
}
